<% if @error_obj.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      The form contains <%= pluralize(@error_obj.errors.count, "error") %>.
    </div>
    
    <ul>
    <% @error_obj.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
 
