<% title = title_of(@tune) %>
<% provide(:title, title) %>
<h1><%= title %></h1>

<div id="music"></div>
<div id="midi"></div>
<div class="abc" ><%=raw content_as_html(@tune) %></div>

<script type="text/javascript">
  var tune = "<%=raw content_as_string(@tune)  %>";
  ABCJS.renderAbc('music', tune, {});
  ABCJS.renderMidi('midi', tune, {});
</script>